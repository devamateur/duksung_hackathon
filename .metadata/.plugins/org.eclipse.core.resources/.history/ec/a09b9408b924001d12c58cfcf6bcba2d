            function displayDetail(){
                var MenuBar=document.getElementById("MenuBar");
                MenuBar.style.display="block";
          var Bar=document.getElementById("v1_9");
          Bar.style.display="none";
            }
    
            function hideDetail(){
                var MenuBar=document.getElementById("MenuBar");
                MenuBar.style.display="none";
          var Bar=document.getElementById("v1_9");
          Bar.style.display="block";
            }
    
            
    
            
