package com.example.demo.model;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

@Builder
@NoArgsConstructor
@AllArgsConstructor
@Data
public class OrderEntity {   // 주문 엔터티
	private String id;   // 이 객체의 id
	private String userId;    // 이 객체를 생성한 유저의 id
	private String foodName;  // 유저가 주문한 음식 이름
	private String orderNum;  // 주문번호(특정 번호 이상이면 대기번호가 됨?)
}
